{
    "name": "linku/api-documentation-bundle",
    "description": "Extension to API-Platform to modify the documenation with more ease",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "Symfony",
        "bundle",
        "API Platform",
        "API Documentation",
        "OpenAPI"
    ],
    "require": {
        "php": "^8.1",
        "api-platform/core": "^3.0|^4.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/http-foundation": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/routing": "^6.0|^7.0",
        "symfony/config": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Linku\\ApiDocumentationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linku\\ApiDocumentationBundle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
